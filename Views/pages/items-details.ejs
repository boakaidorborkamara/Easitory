<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); -%>
</head>
<body>
    <header >
        <%- include('../partials/nav'); -%>
    </header>
    <main class="bg-light">
      
        <!-- inventory overview section  -->
        <section class="container my-5 ">
            <div class="row">
                <img src="https://images.pexels.com/photos/1775043/pexels-photo-1775043.jpeg?auto=compress&cs=tinysrgb&w=1260&h=750&dpr=1"
                    alt="" class="col" style="width: 60%;">
                <div class="col">
                    <h2>Item Name</h2>
                    <p>This is the item description.</p>
                    <table class="table table-borderless">
                        
                        <tbody>
                            <tr>
                                <th scope="row">Category:</th>
                                <td>Frozen</td>
                            </tr>
                            <tr>
                            <th scope="row">Price:</th>
                            <td>4.5 per ea</td>
                            </tr>
                            <tr>
                                <th scope="row">Daily Average Usage:</th>
                                <td>200</td>
                            </tr>
                            <tr>
                                <th scope="row">Quantity Available:</th>
                                <td>1700</td>
                            </tr>
                        </tbody>
                    </table>
                    <div class="row gap-2 p-4">
                        <a href="#" class="btn btn-success rounded-0 col">Edit</a>
                        <a href="#" class="btn btn-danger rounded-0 col">Delete</a>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <footer class="py-5 text-center bg-light">
        <%- include('../partials/footer'); -%>
    </footer>
    
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
    crossorigin="anonymous"></script>
</html>